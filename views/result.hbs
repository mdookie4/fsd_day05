
{{#if hasContent}}
    {{#each news}}
        <div class="row"><h2>{{this.title}}</h2></div>
        <div class="row"><img src="{{this.image}}" alt="news image"></div>
        <div class="row"><h3>{{this.summary}}</h3></div>
        <div class="row">
            <div class="col"><h4>{{this.timestamp}}</h4></div>
            <div class="col"><a href="{{this.urlLink}}"></a></div>
        </div>
    {{/each}}
{{else}}
    <div class="row">No news found</div>
{{/if}}
